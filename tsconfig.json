{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/**/*",
    "src/**/*.vue"
  ]
}
